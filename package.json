{
    "name": "@perplexfi/sdk",
    "version": "0.0.1",
    "author": "Perplex <tech@perplex.finance>",
    "license": "MIT",
    "description": "PerplexFi SDK",
    "repository": {
        "type": "git",
        "url": "https://github.com/PerplexFi/sdk.git"
    },
    "files": [
        "./dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build": "rimraf dist && npm run build:cjs && npm run build:esm",
        "build:cjs": "tsc -p configs/tsconfig.cjs.json",
        "build:esm": "rollup --config configs/rollup.config.mjs",
        "test": "echo 'No test available yet...'"
    },
    "dependencies": {
        "@permaweb/aoconnect": "^0.0.59",
        "graphql-ws": "^5.16.0",
        "isomorphic-ws": "^5.0.0",
        "ws": "^8.18.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@rollup/plugin-typescript": "^12.1.1",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.7.5",
        "@types/ws": "^8.5.12",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.10.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "rollup": "^4.24.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.1"
    }
}
